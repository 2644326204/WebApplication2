
@{
    ViewBag.Title = "center6";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Basic LinkButton - jQuery EasyUI Demo</title>
    <link rel="stylesheet" type="text/css" href="~/Content/jquery-easyui-1.7.0/themes/default/easyui.css">
    <link rel="stylesheet" type="text/css" href="~/Content/jquery-easyui-1.7.0/themes/icon.css">
    <link rel="stylesheet" type="text/css" href="~/Content/jquery-easyui-1.7.0/themes/color.css">
    <link rel="stylesheet" type="text/css" href="~/Content/jquery-easyui-1.7.0/demo/demo.css">
    <script type="text/javascript" src="~/Content/jquery-easyui-1.7.0/jquery.min.js"></script>
    <script type="text/javascript" src="~/Content/jquery-easyui-1.7.0/jquery.easyui.min.js"></script>

</head>
<body style="padding:0px">
    <center><div class='easyui-panel' title='condition' style='width:100%;max-width:400px;padding:30px 60px;'><div style='margin-bottom:20px'><input class='easyui-textbox' label='像素:' labelPosition='top' style='width:100%;'></div><div style='margin-bottom:20px'><input class='easyui-textbox' label='dpi:' labelPosition='top' style='width:100%;'></div><div style='margin-bottom:20px'><input id='picthingwidth' class='easyui-textbox' label='图片物体大小:' labelPosition='top' style='width:100%;'></div><div style='margin-bottom:20px'><input id='thingwidth' class='easyui-textbox' label='物理物体大小:' labelPosition='top' style='width:100%;'></div><div style='margin-bottom:20px'><input id='jiaoju' class='easyui-textbox' label='照相机焦距:' labelPosition='top' style='width:100%;'></div><div><a onclick='clicksubmitdata()' style='width:100%;height:32px'>提交数据返回结果</a></div></div></center>
    <script>
        function clicksubmitdata() {
            //要得到真实边长a,a1
            var picthingwidth = document.getElementById("picthingwidth").value;
            var thingwidth = document.getElementById("thingwidth").value;
            var a = thingwidth / picthingwidth;
            var distance = document.getElementById("jiaoju").value;
            var a1 = distance * a;
            alert(a1);


        }
    </script>
</body>
</html>

